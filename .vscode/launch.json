{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch mugOS",
			"type": "cppdbg",
			"preLaunchTask": "Native build",
			"request": "launch",
			"program": "qemu-system-i386",
			"args": ["-fda", "${workspaceFolder}/build/floppy.img"],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"linux": {
				"preLaunchTask": "Native build",
				"program": "/usr/bin/qemu-system-i386",
				"targetArchitecture": "x64",
				"MIMode": "gdb"
			},
			"osx": {
				"preLaunchTask": "Docker build",
				"program": "/opt/homebrew/bin/qemu-system-i386",
				"targetArchitecture": "arm64",
				"MIMode": "lldb"
			},
		},
		{
			"name": "Tools/FAT/Tests",
			"type": "cppdbg",
			"preLaunchTask": "Native build",
			"request": "launch",
			"program": "${workspaceFolder}/build/tools/fat/tests",
			"args": ["${workspaceFolder}/build/floppy.img", "TEST    TXT"],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"linux": {
				"preLaunchTask": "Native build",
				"targetArchitecture": "x64",
				"MIMode": "gdb"
			},
			"osx": {
				"preLaunchTask": "Docker build",
				"targetArchitecture": "arm64",
				"MIMode": "lldb"
			},
		},
	]
}
